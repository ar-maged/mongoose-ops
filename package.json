{
  "name": "mongoose-ops",
  "version": "0.1.0",
  "description": "Straightforward MongoDB CRUD operations over Mongoose",
  "main": "dist/index.js",
  "types": "dist/types/index.d.js",
  "scripts": {
    "start": "node dist/index.js",
    "start-watch": "nodemon --no-stdin --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "test": "jest",
    "test-watch": "jest --watch",
    "build": "tsc",
    "prepublish": "npm build"
  },
  "peerDependencies": {
    "mongoose": "^5.9.15"
  },
  "dependencies": {
    "flat": "^5.0.0",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@types/flat": "^5.0.1",
    "@types/jest": "^25.2.3",
    "@types/mongoose": "^5.7.21",
    "@types/ramda": "^0.27.6",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.4",
    "mongodb-memory-server": "^6.6.1",
    "mongoose": "^5.9.15",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,md}": "prettier --write"
  },
  "license": "CC0-1.0",
  "author": {
    "name": "Abdelrahman Abdelhafez",
    "email": "abdelrahman@maged.rocks",
    "url": "https://maged.rocks"
  },
  "keywords": [
    "mongodb",
    "mongoose",
    "crud"
  ],
  "homepage": "https://github.com/ar-maged/mongoose-ops",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ar-maged/mongoose-ops.git"
  },
  "bugs": {
    "url": "https://github.com/ar-maged/mongoose-ops/issues"
  }
